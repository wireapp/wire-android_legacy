<androidx.preference.PreferenceScreen xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:android="http://schemas.android.com/apk/res/android">
    <PreferenceCategory
        android:title="@string/pref_account_details_category_title"
        app:iconSpaceReserved="false">
        <EditTextPreference
            android:key="@string/pref_key_name"
            android:title=" "
            android:summary="@string/pref_account_name_title"
            android:dialogTitle="@string/pref_account_edit_name_title"
            app:iconSpaceReserved="false" />
        <EditTextPreference
            android:key="@string/pref_key_username"
            android:title=" "
            android:summary="@string/pref_account_username_title"
            app:iconSpaceReserved="false" />
        <EditTextPreference
            android:key="@string/pref_key_email"
            android:title=" "
            android:summary="@string/pref_account_email_title"
            app:iconSpaceReserved="false" />
        <EditTextPreference
            android:key="@string/pref_key_phone"
            android:title=" "
            android:summary="@string/pref_account_phone_title"
            app:iconSpaceReserved="false" />
    </PreferenceCategory>
    <PreferenceCategory
        android:title="@string/pref_account_appearance_category_title"
        app:iconSpaceReserved="false">
        <Preference
            android:key=""
            android:title="@string/pref_account_picture_title"
            app:icon="@drawable/conversation_badge_red"
            app:iconSpaceReserved="true" />
        <Preference
            android:key=""
            android:title="@string/pref_account_color_title"
            app:icon="@drawable/conversation_badge_red"
            app:iconSpaceReserved="true" />
    </PreferenceCategory>
    <PreferenceCategory
        android:title="@string/pref__account_privacy"
        app:iconSpaceReserved="false">
        <SwitchPreferenceCompat
            android:key=""
            app:title="@string/pref__account_send_read_receipts_title"
            app:summary="@string/pref__account_send_read_receipts_summary"
            app:iconSpaceReserved="false" />
    </PreferenceCategory>
    <PreferenceCategory
        android:title="@string/pref_account_personal_information_category_title"
        app:iconSpaceReserved="false">
        <Preference
            android:key=""
            app:title="@string/pref_account_data_usage_permissions"
            app:iconSpaceReserved="false" />
    </PreferenceCategory>
    <PreferenceCategory
        android:title="@string/pref_account_conversations_category_title"
        app:iconSpaceReserved="false">
        <Preference
            android:key=""
            app:title="@string/pref_account_backup_title"
            app:iconSpaceReserved="false" />
    </PreferenceCategory>
    <PreferenceCategory
        android:title="@string/pref_account_actions_category_title"
        app:iconSpaceReserved="false">
        <Preference
            android:key=""
            app:title="@string/pref_account_password_title"
            app:iconSpaceReserved="false" />
        <Preference
            android:key=""
            app:title="@string/pref_account_sign_out_title"
            app:iconSpaceReserved="false" />
        <Preference
            android:key=""
            app:title="@string/pref_account_delete_title"
            app:iconSpaceReserved="false" />
    </PreferenceCategory>
</androidx.preference.PreferenceScreen>

